{
    "env": {
        "browser": true,
        "amd": true,
        "node": true
    },
    "parser": "esprima",
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "prettier/prettier": "warn",
        "eqeqeq": "warn",
        "curly": "error",
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }]
    }
}